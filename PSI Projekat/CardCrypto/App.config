<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
    <add key="ExtensionKeyChrome" value="kbmjapfccpkonkbkdkacekebekdmpkmm" />
    <add key="ExtensionKeyFirefox" value="messenger.cardreader@brzeboljejeftinije.rs" />
    <add key="ExtensionName" value="brzeboljejeftinije.messenger.cardreader" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
  <log4net>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="FileAppender" />
    </root>
    <appender name="FileAppender" type="log4net.Appender.FileAppender">
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <param name="File" value="${USERPROFILE}\RSIDCrypto\Logs\events.log" />
      <param name="Encoding" value="utf-8" />
      <param name="AppendToFile" value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%date [%thread] %-5level %logger+%method - %message%newline" />
      </layout>
    </appender>
  </log4net>
</configuration>